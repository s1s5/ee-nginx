location {{ location }} {{ "{" }}
    {% if let Some(domain) = domain %}
      proxy_pass {{ domain }}{{ alias }};
    {% else %}
      alias {{ alias }};
      index  index.html index.htm;
    {% endif %}
    {% if fallback %}
      try_files $uri $uri/ / =404;
    {% endif %}
    {% if let Some(basic_auth) = basic_auth %}
      auth_basic "Authorization required";
      auth_basic_user_file {{ basic_auth }};
    {% endif %}
{{ "}" }}